						{{/each}}
					</ul>

					<h3>Styles</h3>
					<ul class="nsw-docs__list nsw-docs__list--4">
						{{#each collections.stylesnav}}
						<li><a href="{{change-extention path}}">{{title}}</a></li>
						{{/each}}
					</ul>

					<h3>Demo pages</h3>
					<ul class="nsw-docs__list nsw-docs__list--2">
						{{#each collections.templatesnav}}
