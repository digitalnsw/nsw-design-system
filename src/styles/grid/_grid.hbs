<div class="nsw-grid">
{{#if simple}}
  {{#each cols}}
  <div class="nsw-col">
    <!-- START: Column content -->
    <div class="nsw-ds-example-box"></div>
    <!-- END: Column content -->
  </div>
  {{/each}}
{{else}}
  {{#each cols}}
  <div class="nsw-col {{this}}">
    <!-- START: Column content -->
    <div class="nsw-ds-example-box"></div>
    <!-- END: Column content -->
  </div>
  {{/each}}
{{/if}}
</div>