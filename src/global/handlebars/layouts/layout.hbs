      {{>_docs-header}}
    </div>
  </div>
  <div class="nsw-docs__wrapper">
    <div class="nsw-docs__nav">
      <div class="nsw-docs__nav-wrapper">
        {{>_docs-nav}}
      </div>
    </div>
    <div class="nsw-docs__main{{#if width}} nsw-docs__main--{{width}}{{/if}}">
      {{#unless home}}
      {{>_docs-intro}}
      {{/unless}}
      {{#if tabs}}
{{>_docs-tabs}}
      {{else}}
