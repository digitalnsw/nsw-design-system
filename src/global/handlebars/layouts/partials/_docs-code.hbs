<div class="nsw-docs-code">
{{#unless open}}
  <button class="nsw-link nsw-link--button nsw-link--icon nsw-small js-code-button">
    <span>Show code</span>
    <span class="material-symbols-outlined nsw-material-symbols-outlined" focusable="false" aria-hidden="true">keyboard_arrow_down</span>
  </button>
{{/unless}}
  <div class="nsw-docs-code__content{{#if open}} active{{/if}}">
    <button type="button" class="nsw-button nsw-button--dark-outline-solid nsw-button--flex nsw-button--small js-code-copy">
      <span>Copy</span>
      <span class="material-symbols-outlined nsw-material-symbols-outlined" focusable="false" aria-hidden="true">content_copy</span>
    </button>
    <div class="nsw-docs-code__copy" focusable="false" aria-hidden="true" tabindex="-1"><script>document.write((`{{> @partial-block }}`).replace(/</g, "&lt;").replace(/>/g, "&gt;"));</script></div>
    <div class="nsw-docs-code__wrapper">
<pre><code class="{{#if code}}{{code}}{{else}}html{{/if}}"><script>document.write((`{{> @partial-block }}`).replace(/</g, "&lt;").replace(/>/g, "&gt;").replace(/"/g, "&quot;").replace(/'/g, "&#039;"));</script></code></pre>
    </div>
  </div>
</div>
