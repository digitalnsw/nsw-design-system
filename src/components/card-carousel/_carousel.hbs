{{#if default}}<div class="nsw-carousel js-carousel" data-description="Highlighted latest news"{{#if loop}} data-loop="on" data-navigation="off"{{/if}}{{#if drag}} data-drag="on"{{/if}}{{#if pagination}} data-navigation-pagination="on"{{/if}}{{#if counter}} data-loop="off" data-navigation="off"{{/if}}>
  <p class="sr-only">Carousel items</p>
  <div class="nsw-carousel__wrapper js-carousel__wrapper">
    <ol class="nsw-carousel__list">
      {{#each cards}}<li class="nsw-carousel__item">
      <div class="nsw-card">
        {{#if img}}
        <div class="nsw-card__image">
          <img src="{{img}}" alt="{{heading}}">
        </div>
        {{/if}}
        <div class="nsw-card__content">
          <div class="nsw-card__title">
            <a href="https://www.digital.nsw.gov.au">{{heading}}</a>
          </div>{{#if copy}}
          <div class="nsw-card__copy">
            <p>{{copy}}</p>
          </div>{{/if}}
          <span class="material-symbols-outlined nsw-material-symbols-outlined" focusable="false" aria-hidden="true">east</span>
        </div>
      </div>
      </li>{{/each}}
    </ol>
  </div>{{#unless counter}}
    {{#if title}}
      <div class="nsw-carousel__header">
        <div class="nsw-carousel__title">
          <h2>{{{title}}}</h2>
        </div>
        <nav aria-label="Carousel controls">
          <ul>
            <li>
              <button aria-label="Show previous items" class="nsw-carousel__control nsw-carousel__control--prev js-carousel__control">
                <svg class="nsw-icon" viewBox="0 0 20 20">
                  <title>Show previous items</title>
                  <polyline points="13 2 5 10 13 18" fill="none" stroke="currentColor" stroke-linecap="square" stroke-linejoin="square" stroke-width="2" />
                </svg>
              </button>
            </li>
            <li>
              <button aria-label="Show next items" class="nsw-carousel__control nsw-carousel__control--next js-carousel__control">
                <svg class="nsw-icon" viewBox="0 0 20 20">
                  <title>Show next items</title>
                  <polyline points="7 2 15 10 7 18" fill="none" stroke="currentColor" stroke-linecap="square" stroke-linejoin="square" stroke-width="2" />
                </svg>
              </button>
            </li>
          </ul>
        </nav>
      </div>
    {{else}}
      <nav>
        <ul>
          <li>
            <button aria-label="Show previous items" class="nsw-carousel__control nsw-carousel__control--prev js-carousel__control">
              <svg class="nsw-icon" viewBox="0 0 20 20">
                <title>Show previous items</title>
                <polyline points="13 2 5 10 13 18" fill="none" stroke="currentColor" stroke-linecap="square" stroke-linejoin="square" stroke-width="2" />
              </svg>
            </button>
          </li>
          <li>
            <button aria-label="Show next items" class="nsw-carousel__control nsw-carousel__control--next js-carousel__control">
              <svg class="nsw-icon" viewBox="0 0 20 20">
                <title>Show next items</title>
                <polyline points="7 2 15 10 7 18" fill="none" stroke="currentColor" stroke-linecap="square" stroke-linejoin="square" stroke-width="2" />
              </svg>
            </button>
          </li>
        </ul>
      </nav>
    {{/if}}
  {{/unless}}{{#if counter}}
  <div class="nsw-carousel__counter">
    <p><i class="js-carousel__counter"></i> / <i class="js-carousel__counter-tot"></i></p>
    <nav>
      <ul>
        <li>
          <button aria-label="Show previous items" class="nsw-carousel__control nsw-carousel__control--prev js-carousel__control">
            <svg class="nsw-icon" viewBox="0 0 20 20">
              <title>Show previous items</title>
              <polyline points="13 2 5 10 13 18" fill="none" stroke="currentColor" stroke-linecap="square" stroke-linejoin="square" stroke-width="2" />
            </svg>
          </button>
        </li>
        <li>
          <button aria-label="Show next items" class="nsw-carousel__control nsw-carousel__control--next js-carousel__control">
            <svg class="nsw-icon" viewBox="0 0 20 20">
              <title>Show next items</title>
              <polyline points="7 2 15 10 7 18" fill="none" stroke="currentColor" stroke-linecap="square" stroke-linejoin="square" stroke-width="2" />
            </svg>
          </button>
        </li>
      </ul>
    </nav>
  </div>{{/if}}
</div>{{/if}}
{{#if view-more}}
<a class="nsw-carousel--view-more" href="{{view-more}}" target="_blank" rel="noopener">
  <span>View more</span>
  <span class="material-symbols-outlined nsw-material-symbols-outlined" focusable="false" aria-hidden="true">east</span>
</a>
{{/if}}