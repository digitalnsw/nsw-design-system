          <h2 class="nsw-section-title">Text block</h2>
          <div class="nsw-grid">
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-text-1}}
            </div>
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-text-2}}
            </div>
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-text-3}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="nsw-section--light-10">
    <div class="nsw-container">
      <div class="nsw-page-layout">
        <div class="nsw-page-layout__main">
          <h2 class="nsw-section-title">Icon block</h2>
          <div class="nsw-grid">
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-icon-1}}
