          <h2 class="nsw-section-title">Image block</h2>
          <div class="nsw-grid">
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-image-1}}
            </div>
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-image-2}}
            </div>
            <div class="nsw-col nsw-col-md-4">
              {{>_content-block model.content-block-image-3}}
            </div>
          </div>
        </div>
      </div>
