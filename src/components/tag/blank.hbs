---
title: Tags
width: narrow
page: true
model:
  link: json/tag-link.json
  text: json/tag-text.json
  checkbox: json/tag-checkbox.json
---
{{#>_layout-container}}
<div class="nsw-docs__section nsw-docs__section--spaced">
  <div class="nsw-list nsw-list--8">
    {{>_tag model.link}}
  </div>
</div>
<div class="nsw-docs__section nsw-docs__section--spaced">
  <div class="nsw-list nsw-list--8">
    {{>_tag model.text}}
  </div>
</div>
<div class="nsw-docs__section nsw-docs__section--spaced">
  <div class="nsw-list nsw-list--8">
    {{>_tag model.checkbox}}
  </div>
</div>
{{/_layout-container}}
