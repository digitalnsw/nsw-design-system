<fieldset class="nsw-form__date">
  <legend>
    <span class="nsw-form__label{{#if required}} nsw-form__required{{/if}}">{{label}}{{#if required}}<span class="sr-only"> (required)</span>{{/if}}</span>{{#if helper-text}}
    <span id="{{id}}-helper-text" class="nsw-form__helper">{{helper-text}}</span>{{/if}}
  </legend>
  <div class="nsw-form__date-wrapper">
    <div class="nsw-form__date-input">
      <label for="{{id}}-day" class="nsw-form__label nsw-form__label--small">Day</label>
      <input {{#if value-day}}value="{{value-day}}"{{/if}} type="text" maxlength="2" id="{{id}}-day" name="{{id}}-day" class="nsw-form__input"{{#if error-text}} aria-invalid="true"{{/if}} {{#if disabled}}disabled={{disabled}}{{/if}} {{#if readonly}}readonly="{{readonly}}"{{/if}}>
    </div>
    <div class="nsw-form__date-input">
      <label for="{{id}}-month" class="nsw-form__label nsw-form__label--small">Month</label>
      <input {{#if value-month}}value="{{value-month}}"{{/if}} type="text" maxlength="2" id="{{id}}-month" name="{{id}}-month" class="nsw-form__input"{{#if error-text}} aria-invalid="true"{{/if}} {{#if disabled}}disabled={{disabled}}{{/if}} {{#if readonly}}readonly="{{readonly}}"{{/if}}>
    </div>
    <div class="nsw-form__date-input nsw-form__date-input--large">
      <label for="{{id}}-year" class="nsw-form__label nsw-form__label--small">Year</label>
      <input {{#if value-year}}value="{{value-year}}"{{/if}} type="text" maxlength="4" id="{{id}}-year" name="{{id}}-year" class="nsw-form__input"{{#if error-text}} aria-invalid="true"{{/if}} {{#if disabled}}disabled={{disabled}}{{/if}} {{#if readonly}}readonly="{{readonly}}"{{/if}}>
    </div>
  </div>
</fieldset>
{{#if error-text}}
<span id="{{id}}-error-text" class="nsw-form__helper nsw-form__helper--error"><span class="material-symbols-outlined nsw-material-symbols-outlined" focusable="false" aria-hidden="true">cancel</span>{{error-text}}</span>
{{/if}}{{#if valid-text}}
<span id="{{id}}-valid-text" class="nsw-form__helper nsw-form__helper--valid"><span class="material-symbols-outlined nsw-material-symbols-outlined" focusable="false" aria-hidden="true">check_circle</span>{{valid-text}}</span>
{{/if}}
