<h3>Default</h3>

<h4>Text Fields</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    {{>_text-input
      id="form-text-2"
      label="Name"
      helper-text="As it appears on your identity document"
    }}
  </div>
  <div class="nsw-form__group">
    {{>_textarea
      id="form-textarea-2"
      label="Comment"
      helper-text="Maximum 500 characters"
    }}
  </div>
</div>
{{/_docs-example}}

<h4>Input groups</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    {{>_input-group
      id="form-input-group-1"
      label="Search"
    }}
  </div>
  <div class="nsw-form__group">
    {{>_input-group
      id="form-input-group-icon-1"
      label="Search"
      icon="search"
    }}
  </div>
  <div class="nsw-form__group">
    {{>_input-group
      id="form-input-group-icon-2"
      label="Search"
      icon="search"
      white="true"
    }}
  </div>
</div>
{{/_docs-example}}

<h4>Predictive list</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    <label class="nsw-form__label" for="form-predictive-text-1">Suburb</label>
    <div class="nsw-form__predictive">
      <input class="nsw-form__input" type="text" id="form-predictive-text-1" name="form-predictive-text-1" value="Horn" aria-autocomplete="list" autocomplete="off">
      <ul class="nsw-form__predictive-list">
        <li><a href="#"><span>Horn</span>sby</a></li>
        <li><a href="#"><span>Horn</span>sby Heights</a></li>
        <li><a href="#"><span>Horn</span>ingsea Park</a></li>
      </ul>
    </div>
  </div>
  <br><br><br><br><br><br>
  <div class="nsw-form__group">
    <div class="nsw-form__predictive">
      {{>_input-group
        id="form-predictive-group-1"
        label="Search"
        value="Tran"
        predictive=true
      }}
      <ul class="nsw-form__predictive-list">
        <li><a href="#"><span>Tran</span>sport</a></li>
        <li><a href="#">Road <span>Tran</span>sport</a></li>
        <li><a href="#">Public <span>Tran</span>sport</a></li>
      </ul>
    </div>
  </div>
  <br><br><br><br><br><br>
  <div class="nsw-form__group">
    <div class="nsw-form__predictive">
      {{>_input-group
        id="form-predictive-group-icon-1"
        label="Search"
        value="Govern"
        icon="search"
        large=true
        predictive=true
      }}
      <ul class="nsw-form__predictive-list nsw-form__predictive-list--large">
        <li><a href="#"><span>Govern</span>ment</a></li>
        <li><a href="#"><span>Govern</span>ment payments</a></li>
        <li><a href="#"><span>Govern</span>ance</a></li>
      </ul>
    </div>
  </div>
  <br><br><br><br><br><br>
</div>
{{/_docs-example}}

<h4>Date</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    {{>_date
      id="form-date-2"
      label="Date of birth"
      helper-text="For example 08 12 1990"
    }}
  </div>
</div>
{{/_docs-example}}

<h4>Select</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    {{>_select model.select
      id="form-select-2"
      label="Region"
      helper-text="Your region of residence"
    }}
  </div>
</div>
{{/_docs-example}}

<h4>Checkboxes</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    <fieldset class="nsw-form__fieldset">
      <legend>
        <span class="nsw-form__legend">Category</span>
        <span class="nsw-form__helper">Select all that applies to you</span>
      </legend>
      {{>_checkbox
        id="form-checkbox-multi-2-a"
        name="form-checkbox-multi-2"
        label="Customer Service"
      }}
      {{>_checkbox
        id="form-checkbox-multi-2-b"
        name="form-checkbox-multi-2"
        label="Communities and Justice"
      }}
    </fieldset>
  </div>
</div>
{{/_docs-example}}

<h4>Radio buttons</h4>
{{#>_docs-example separated=true}}
<div class="nsw-form">
  <div class="nsw-form__group">
    <fieldset class="nsw-form__fieldset">
      <legend>
        <span class="nsw-form__legend">Topic</span>
        <span class="nsw-form__helper">Select closest match</span>
      </legend>
      {{>_radio
        id="form-radio-2-a"
        name="form-radio-2"
        label="NSW Government"
      }}
      {{>_radio
        id="form-radio-2-b"
        name="form-radio-2"
        label="Business and Economy"
      }}
    </fieldset>
  </div>
</div>
{{/_docs-example}}