<footer class="nsw-footer{{#if theme}} nsw-footer--{{theme}}{{/if}}">
  {{#if sectionLinks}}
  <div class="nsw-footer__upper">
    <div class="nsw-container">
      {{#each sectionLinks}}
        <div class="nsw-footer__group">
          <div class="nsw-footer__heading"><a href="{{url}}">{{title}}</a></div>
          <ul>
            {{#each links}}
            <li><a href="{{url}}">{{text}}</a></li>
            {{/each}}
          </ul>
        </div>
      {{/each}}
    </div>
  </div>
  {{/if}}
  <div class="nsw-footer__lower">
    <div class="nsw-container">
      <p class="nsw-footer__acknowledgement">{{acknowledgementOfCountry}}</p>
      {{#unless simple}}
      <hr>{{#if social}}
      <div class="nsw-footer__links">{{/if}}
      <ul>
        {{#each footerLinks}}
        <li><a href="{{url}}">{{text}}</a></li>
        {{/each}}
      </ul>{{#if social}}
      <div class="nsw-footer__social">
        <a class="nsw-icon-button" href="#" target="_blank" rel="noopener" aria-label="LinkedIn">
          {{#if-equals theme "light"}}
