<div class="nsw-link-list">
  {{#if title}}
  <div class="nsw-link-list__title">{{title}}</div>
  {{/if}}
  <ul>
    {{#each list}}
    <li>
      <a href="#">
        <span>{{this}}</span>
        <span class="material-icons nsw-material-icons" focusable="false" aria-hidden="true">east</span>
      </a>
    </li>
    {{/each}}
  </ul>
</div>