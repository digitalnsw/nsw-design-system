---
title: Buttons
width: wide
page: true
---

{{#>_layout-container}}
  <!-- Brand Dark -->
  <div class="nsw-list nsw-list--8 nsw-m-bottom-sm">
    {{>_button-link url="#" style="dark" text="Link"}}
    {{>_button-link url="#" style="dark" text="Link icon" icon="upload"}}
    {{>_button type="button" style="dark" text="Button"}}
    {{>_button type="button" style="dark" text="Button link" icon="download" icon-after="true"}}
    {{>_button-input type="button" style="dark" text="Input"}}
    {{>_button-input type="button" style="dark" text="Disabled" isDisabled=true}}
    {{>_button type="button" style="dark" text="Full Width" isFullWidth=true}}
  </div>

  <!-- Brand Dark Outline -->
  <div class="nsw-list nsw-list--8 nsw-m-bottom-sm">
    {{>_button-link url="#" style="dark-outline" text="Link"}}
    {{>_button-link url="#" style="dark-outline" text="Link icon" icon="upload"}}
    {{>_button type="button" style="dark-outline" text="Button"}}
    {{>_button type="button" style="dark-outline" text="Button link" icon="download" icon-after="true"}}
    {{>_button-input type="button" style="dark-outline" text="Input"}}
    {{>_button-input type="button" style="dark-outline" text="Disabled" isDisabled=true}}
    {{>_button type="button" style="dark-outline" text="Full Width" isFullWidth=true}}
  </div>

  <!-- Brand Dark Outline Solid -->
  <div class="nsw-list nsw-list--8 nsw-m-bottom-sm">
    {{>_button-link url="#" style="dark-outline-solid" text="Link"}}
    {{>_button-link url="#" style="dark-outline-solid" text="Link icon" icon="upload"}}
