    {{>_button type="button" style="dark-outline-solid" text="Button link" icon="download" icon-after="true"}}
    {{>_button-input type="button" style="dark-outline-solid" text="Input"}}
    {{>_button-input type="button" style="dark-outline-solid" text="Disabled" isDisabled=true}}
    {{>_button type="button" style="dark-outline-solid" text="Full Width" isFullWidth=true}}
  </div>

  <!-- Brand Light -->
  <div class="nsw-list nsw-list--8 nsw-m-bottom-sm">
    {{>_button-link url="#" style="light" text="Link"}}
    {{>_button-link url="#" style="light" text="Link icon" icon="upload"}}
    {{>_button type="button" style="light" text="Button"}}
    {{>_button type="button" style="light" text="Button link" icon="download" icon-after="true"}}
    {{>_button-input type="button" style="light" text="Input"}}
    {{>_button-input type="button" style="light" text="Disabled" isDisabled=true}}
    {{>_button type="button" style="light" text="Full Width" isFullWidth=true}}
  </div>

  <!-- Danger -->
  <div class="nsw-list nsw-list--8 nsw-m-bottom-sm">
    {{>_button-link url="#" style="danger" text="Link"}}
    {{>_button-link url="#" style="danger" text="Link icon" icon="upload"}}
    {{>_button type="button" style="danger" text="Button"}}
    {{>_button type="button" style="danger" text="Button link" icon="download" icon-after="true"}}
    {{>_button-input type="button" style="danger" text="Input"}}
    {{>_button-input type="button" style="danger" text="Disabled" isDisabled=true}}
    {{>_button type="button" style="danger" text="Full Width" isFullWidth=true}}
  </div>
{{/_layout-container}}

