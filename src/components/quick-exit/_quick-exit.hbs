<a
    class="nsw-quick-exit"
    href="{{#if model.quick-exit.safeUrl}}{{model.quick-exit.safeUrl}}{{else}}https://www.google.com/webhp{{/if}}"
    data-options='{
      "safePageTitle": "{{#if model.quick-exit.safePageTitle}}{{model.quick-exit.safePageTitle}}{{else}}NSW Government{{/if}}",
      "enableEsc": true,
      "enableCloak": true
    }'
    rel="nofollow noopener"
    id="nsw-quick-exit"
    aria-describedby="nsw-quick-exit__desc"
  >
    <div class="nsw-quick-exit__content">
      <span
        id="nsw-quick-exit__desc"
        class="nsw-quick-exit__description-text"
      >
        Leave quickly using this banner or press <kbd>Esc</kbd> 2 times.
      </span>
      <span class="nsw-quick-exit__exit-text">Exit now</span>
    </div>
  </a>